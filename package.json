{
  "name": "ai-resume-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:clean && npm run build:compile",
    "build:clean": "rimraf dist",
    "build:compile": "vite build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:staging": "cross-env NODE_ENV=production VITE_ENV=staging npm run build",
    "build:docker": "docker build -t ai-resume-generator .",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "jest --coverage --coverageReporters=html --coverageReporters=lcov --coverageReporters=text",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "test:e2e:coverage": "nyc --reporter=html --reporter=text cypress run",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:report": "node scripts/generate-test-report.cjs",
    "test:benchmark": "npm run cypress:run -- --spec 'cypress/e2e/benchmark.cy.ts'",
    "test:performance": "npm run cypress:run -- --spec 'cypress/e2e/performance.cy.ts'",
    "test:cross-browser": "npm run cypress:run:chrome && npm run cypress:run:firefox && npm run cypress:run:edge",
    "test:ai-accuracy": "npm run cypress:run -- --spec 'cypress/e2e/ai-accuracy.cy.ts'",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "deploy:staging": "npm run build:staging && npm run deploy:docker:staging",
    "deploy:production": "npm run build:production && npm run deploy:docker:production",
    "deploy:docker:staging": "docker-compose -f docker-compose.staging.yml up -d",
    "deploy:docker:production": "docker-compose -f docker-compose.production.yml up -d",
    "deploy:k8s:staging": "kubectl apply -f k8s/staging/",
    "deploy:k8s:production": "kubectl apply -f k8s/production/",
    "health-check": "curl -f http://localhost/health || exit 1",
    "performance:audit": "lighthouse http://localhost --output=html --output-path=./reports/lighthouse.html",
    "security:audit": "npm audit && npm run security:scan",
    "security:scan": "echo 'Running security scan...' && npm audit --audit-level moderate"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@xenova/transformers": "^2.6.2",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "clsx": "^2.0.0",
    "d3": "^7.8.5",
    "framer-motion": "^10.16.5",
    "html2canvas": "^1.4.1",
    "idb": "^7.1.1",
    "jspdf": "^2.5.1",
    "openai": "^4.20.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@cypress/code-coverage": "^3.14.5",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.10.3",
    "babel-jest": "^30.0.5",
    "chrome-launcher": "^1.2.0",
    "cross-env": "^10.0.0",
    "cypress": "^14.5.3",
    "cypress-axe": "^1.6.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.0",
    "nyc": "^17.1.0",
    "playwright": "^1.54.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.10",
    "start-server-and-test": "^2.0.12",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0"
  }
}
