# 生产环境配置
NODE_ENV=production

# API配置
VITE_API_BASE_URL=https://api.ai-resume.com
VITE_OPENAI_API_URL=https://api.openai.com/v1
VITE_HUGGINGFACE_API_URL=https://huggingface.co

# CDN配置
VITE_CDN_ENABLED=true
VITE_CDN_BASE_URL=https://cdn.ai-resume.com
VITE_STATIC_CDN=https://static.ai-resume.com
VITE_IMAGES_CDN=https://images.ai-resume.com
VITE_FONTS_CDN=https://fonts.ai-resume.com
VITE_MODELS_CDN=https://models.ai-resume.com

# 性能配置
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_PERFORMANCE_MONITORING=true

# 功能开关
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_BACKGROUND_SYNC=true
VITE_ENABLE_PUSH_NOTIFICATIONS=false

# 安全配置
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# 构建配置
VITE_BUILD_ANALYZE=false
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_COMPRESSION=true

# 缓存配置
VITE_CACHE_MAX_AGE=86400
VITE_SW_CACHE_NAME=ai-resume-v1
VITE_API_CACHE_TTL=3600

# 版本信息
VITE_APP_VERSION=1.0.0
VITE_BUILD_TIME=__BUILD_TIME__
VITE_GIT_COMMIT=__GIT_COMMIT__