{
  "extends": "@cypress/code-coverage/use-babelrc",
  "include": [
    "src/**/*.{ts,tsx}"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/test/**/*",
    "src/main.tsx",
    "src/vite-env.d.ts",
    "src/**/*.stories.*",
    "src/**/*.config.*"
  ],
  "reporter": [
    "text",
    "html",
    "lcov",
    "json"
  ],
  "report-dir": "coverage-e2e",
  "check-coverage": true,
  "branches": 90,
  "functions": 90,
  "lines": 90,
  "statements": 90
}